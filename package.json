{
  "name": "visual-ui-mcp-server",
  "version": "3.1.0",
  "description": "MCP Server for visual UI testing and browser automation",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "install-browsers": "npx playwright install",
    "test": "vitest run",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.2",
    "fs-extra": "^11.2.0",
    "mime-types": "^3.0.1",
    "pixelmatch": "^5.3.0",
    "playwright": "^1.40.0",
    "pngjs": "^7.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/fs-extra": "^11.0.0",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^20.0.0",
    "@types/pixelmatch": "^5.2.6",
    "@types/pngjs": "^6.0.0",
    "@vitest/ui": "^3.2.4",
    "express": "^5.1.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
