{
  "name": "visual-ui-mcp-server",
  "version": "4.0.1",
  "description": "MCP Server for visual UI testing and browser automation",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "start": "node dist/index.js",
    "dev": "tsx src/server.ts",
