    "dotenv": "^17.2.3",
    "fs-extra": "^11.2.0",
    "mime-types": "^3.0.1",
    "pixelmatch": "^5.3.0",
    "playwright": "^1.40.0",
    "pngjs": "^7.0.0",
    "winston": "^3.18.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@types/cross-spawn": "^6.0.6",
    "@types/express": "^5.0.3",
    "@types/fs-extra": "^11.0.0",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^20.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "cross-spawn": "^7.0.6",
    "express": "^5.1.0",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0",
